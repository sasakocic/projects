<p style="color: green"><%= notice %></p>

<table class="table">
  <thead>
  <tr>
    <th scope="col">#</th>
    <th scope="col">Name</th>
    <th scope="col">E-mail</th>
    <th scope="col">Description</th>
    <th scope="col">User</th>
    <th scope="col">Links</th>
  </tr>
  </thead>
  <tbody>
  <%= render @member %>
  </tbody>
</table>

<div>
  <%= link_to "Edit this member", edit_member_path(@member) %> |
  <%= link_to "Back to members", members_path %>

  <%= button_to "Destroy this member", @member, method: :delete, class: "btn btn-danger ml-3" %>
</div>
